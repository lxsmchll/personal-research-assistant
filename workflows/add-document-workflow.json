    {
      "parameters": {
        "jsCode": "// Extract file URL from Slack command\nconst slackPayload = $input.item.json;\nconst fileUrl = slackPayload.text?.trim() || '';\nconst userId = slackPayload.user_id;\nconst userName = slackPayload.user_name;\n\n// Validate URL\nif (!fileUrl) {\n  return {\n    json: {\n      error: true,\n      message: 'Please provide a file URL. Usage: /add-doc <file_url>',\n      userId,\n      userName\n    }\n  };\n}\n\n// Check if PDF\nif (!fileUrl.toLowerCase().endsWith('.pdf') && !fileUrl.includes('pdf')) {\n  return {\n    json: {\n      error: true,\n      message: 'Currently only PDF files are supported',\n      userId,\n      userName\n    }\n  };\n}\n\nreturn {\n  json: {\n    fileUrl,\n    userId,\n    userName,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -304,
        704
      ],
      "id": "f7a3d2d3-6503-41a8-850b-b1ba99e55438",
      "name": "Parse Slack Input"
    },
    {
      "parameters": {
        "jsCode": "// Get the extracted text from the \"Extract from File\" node\nconst extractedData = $input.first().json;\n\n// The Extract from File node returns text in the 'text' field\nconst fullText = extractedData.text || '';\n\n// Get file info from previous nodes\nconst fileUrl = $('Parse Slack Input').item.json.fileUrl;\nconst fileName = fileUrl.split('/').pop().replace('.pdf', '');\nconst userName = $('Parse Slack Input').item.json.userName;\n\n// Create title from filename (Extract from File doesn't give us PDF metadata)\nconst title = fileName.replace(/_/g, ' ').replace(/-/g, ' ');\nconst author = 'Unknown'; // Extract from File node doesn't extract PDF metadata\n\n// Count pages (rough estimate - Extract from File doesn't provide page count)\n// We'll estimate based on text length (roughly 3000 chars per page)\nconst pageCount = Math.max(1, Math.ceil(fullText.length / 3000));\n\n// Count words\nconst wordCount = fullText.trim().split(/\\s+/).length;\n\n// Store metadata for later nodes\nreturn {\n  json: {\n    text: fullText.trim(),\n    title: title || 'Untitled Document',\n    author: author,\n    page_count: pageCount,\n    word_count: wordCount,\n    char_count: fullText.length,\n    file_url: fileUrl,\n    added_by: userName,\n    added_date: new Date().toISOString()\n  }\n};"
      },
